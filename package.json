  "main": "app.js",
  "scripts": {
    "test": "jest --passWithNoTests --verbose --colors --config jest.config.js",
    "coverage": "npm test -- --coverage",
    "pre-commit": "run-s \"test\" \"clean\" \"lint\"",
    "pre-push": "run-s \"test\" \"clean\" \"lint\"",
    "dev": "run-s \"lint\" \"dev:server -- {@}\" --",
    "dev:dashboard": "run-s \"lint\" \"dev:server:dashboard -- {@}\" --",
    "dev:server": "webpack-dev-server --mode development -d --config ./config/webpack/webpack.dev.js --watch",
    "dev:server:dashboard": "webpack-dashboard -- webpack-dev-server --mode development -d --config ./config/webpack/webpack.dev.js --watch --open",
    "build": "run-s \"lint\" \"clean\" \"build:webpack -- {@}\" \"build:gulp:postbuild -- {@}\" --",
    "build:gulp:postbuild": "gulp postbuild",
    "build:webpack": "webpack --mode production --config ./config/webpack/webpack.prod.js",
    "reset": "rm -rf dist && rm -rf node_modules && rm package-lock.json",
    "clean": "rm -rf dist",
    "lint": "run-s \"lint:js\" \"lint:css:js\" \"lint:css:jsx\"",
    "lint:css:js": "stylelint ./src/**/*.js",
    "lint:css:jsx": "stylelint ./src/**/*.jsx",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:js:fix": "npm run lint:eslint:fix -- . ",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "doc": "jsdoc -c config/jsdoc/conf.json"
  },
